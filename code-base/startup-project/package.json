    "source-map-support": "0.5.20",
    "supertest": "7.1.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "typescript-eslint": "8.27.0",
    "unplugin-swc": "1.5.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.9"
  }
