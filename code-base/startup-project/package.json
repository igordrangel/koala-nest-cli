{
  "name": "demo",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run swc:copy-files && nest build",
    "swc:copy-files": "npx swc src --copy-files -d dist",
    "start": "nest start",
    "start:dev": "npm run swc:copy-files && nest start --watch",
    "start:debug": "npm run swc:copy-files && nest start --debug --watch",
    "start:prod": "node dist/infra/main",
    "lint": "prettier --end-of-line lf --write . && npx eslint --fix src/**/* && npx eslint --fix .eslintrc.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@koalarx/utils": "3.1.3",
    "@koalarx/nest": "^1.0.0",
    "@nestjs/common": "11.0.12",
    "@nestjs/config": "4.0.1",
    "@nestjs/core": "11.0.12",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/platform-express": "11.0.12",
    "@nestjs/swagger": "11.0.7",
    "@prisma/client": "6.5.0",
    "@scalar/nestjs-api-reference": "0.4.2",
    "cookie-parser": "1.4.6",
    "dotenv": "16.0.3",
    "express-basic-auth": "1.2.1",
    "ioredis": "5.3.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "zod": "3.22.4",
    "zod-validation-error": "1.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@faker-js/faker": "^9.6.0",
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "11.0.12",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.11",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@typescript-eslint/parser": "8.27.0",
